{
  "Unify__AppBaseUrl": "https://www.sbs.strath.ac.uk/linux-app",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Unify": {
    "Identity": {
      "Provider": "Azure",
      "AzureAd": {
        "ProfileService": "MsGraph",
        "Instance": "https://login.microsoftonline.com/",
        "Authority": "https://login.microsoftonline.com/{0}/v2.0",
        "MsGraphBaseUrl": "https://graph.microsoft.com/beta",
        "TenantId": "631e0763-1533-47eb-a5cd-0457bee5944e",
        "Domain": "uni.strath.ac.uk",
        "CallbackPath": "/signin-oidc",
        "SignedOutCallbackPath": "/signout-callback-oidc",
        "ClientId": "NOT-SETUP",
        "ClientSecret": "NOT-SETUP"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "AuthorizationPolicy": "proxyPolicy",
        "Match": {
          "Path": "/proxy/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/proxy"
          }
        ]
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "http://192.168.56.101/"
          }
        }
      }
    }
  }
}